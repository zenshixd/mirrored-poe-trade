{
  "name": "@mirrored-poe-trade/trade-api",
  "version": "0.0.140",
  "module": "src/updater.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/updater.ts",
    "build:updater": "bun build src/updater.ts --outfile=dist/updater.js --target=bun",
    "build:scrapper": "bun build src/scrapper.ts --outfile=dist/scrapper.js --target=bun",
    "build:migrater": "bun build src/migrater.ts --outfile=dist/migrater.js --target=bun",
    "build": "bun build:updater && bun build:scrapper",
    "push": "bun ./tools/docker-push.ts",
    "prisma:generate": "bun prisma:generate:mpt && bun prisma:generate:scrapper",
    "prisma:generate:mpt": "prisma generate --schema prisma/mpt/schema.prisma",
    "prisma:generate:scrapper": "prisma generate --schema prisma/scrapper/schema.prisma",
    "prisma:migrate:mpt": "prisma migrate dev --schema prisma/mpt/schema.prisma",
    "prisma:migrate:scrapper": "prisma migrate dev --schema prisma/scrapper/schema.prisma"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prisma": "5.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.418.0",
    "@aws-sdk/client-ecs": "^3.484.0",
    "@aws-sdk/client-s3": "^3.484.0",
    "@aws-sdk/lib-dynamodb": "^3.418.0",
    "@prisma/client": "5.8.1",
    "aws-xray-sdk": "^3.5.3",
    "dynamodb-toolbox": "^0.8.5",
    "elysia": "0.8.9"
  }
}